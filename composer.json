{
  "name": "devkhris/laravel-friendships",
  "description": "This package gives Eloquent models the ability to manage friendship relations, this package is a fork from hootlex/laravel-friendships.",
  "keywords": [
    "laravel",
    "friendships",
    "friend-system",
    "friends",
    "eloquent"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "DevKhris",
      "email": "contact@devkhris.com"
    }
  ],
  "autoload-dev": {
    "classmap": [
      "vendor/laravel/laravel/tests/TestCase.php"
    ],
    "files": [
      "tests/helpers.php"
    ],
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.0.0"
  },
  "require-dev": {
    "laravel/laravel": "^8.5",
    "phpunit/phpunit": "^9.5",
    "fzaninotto/faker": "^1.9",
    "mockery/mockery": "^1.4",
    "doctrine/dbal": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Hootlex\\Friendships\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hootlex\\Friendships\\FriendshipsServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
